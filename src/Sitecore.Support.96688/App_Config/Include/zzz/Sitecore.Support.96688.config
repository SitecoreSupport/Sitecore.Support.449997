<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore>
    <contentSearch>
      <indexConfigurations>
        <indexUpdateStrategies>
          <intervalAsyncCore>
            <patch:attribute name="type" value="Sitecore.Support.ContentSearch.Maintenance.Strategies.IntervalAsynchronousStrategy, Sitecore.Support.96688"/>
          </intervalAsyncCore>
          <intervalAsyncMaster>
            <patch:attribute name="type" value="Sitecore.Support.ContentSearch.Maintenance.Strategies.IntervalAsynchronousStrategy, Sitecore.Support.96688"/>
          </intervalAsyncMaster>
          <onPublishEndAsync>
            <patch:attribute name="type" value="Sitecore.Support.ContentSearch.Maintenance.Strategies.OnPublishEndAsynchronousStrategy, Sitecore.Support.96688"/>
          </onPublishEndAsync>
          <onPublishEndAsyncSingleInstance singleInstance="true">
            <patch:attribute name="type" value="Sitecore.Support.ContentSearch.Maintenance.Strategies.OnPublishEndAsynchronousSingleInstanceStrategy, Sitecore.Support.96688"/>
          </onPublishEndAsyncSingleInstance>
        </indexUpdateStrategies>
      </indexConfigurations>
    </contentSearch>
  </sitecore>
</configuration>